<section class="contact-us no-padding-bottom" id="contact-us">
     	<div class="container">
        	<!-- BEGIN HEADING -->
        	<h1 class="orange text-center">客户留言填写</h1>
            <p class="text-center">请在此输入留言内容，我们会尽快与您联系。</p>
            <!-- END HEADING -->
            <div class="form rollIn animated">
            <!-- BEGIN FORM -->
            <form id="contact_form" method="post" onsubmit="return false">
            <div class="row">
                <div class="col-md-6">
                    <label>姓名:</label>
                    <input type="text" name="name" id="name">
                </div>
                <div class="col-md-6">
                    <label>地址</label>
                    <input type="text" name="email" id="email">
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <label>电话</label>
                    <input type="text" name="company" id="company">
                </div>
                <div class="col-md-6">
                    <label>是否面谈</label>
                    <input type="text" name="website" id="website">
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                	<div class="message">
                        <label>需求描述:</label>
                        <textarea name="message" id="message"></textarea>
                        <input type="submit" value="提交信息" class="btn btn-orange" id="btn_submit" onclick="proceed();">
                    </div>
                </div>
            </div>
            </form>
            <!-- END FORM -->
            </div>
        </div>
        <div class="clearfix"></div>
      
     </section>