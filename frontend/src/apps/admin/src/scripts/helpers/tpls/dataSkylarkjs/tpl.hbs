<div class="skylark">
    <div class="theme">
        <h1 class="col-xs-12 col-md-offset-3 col-md-12 title">{{title1}}</h1>
        <p class="col-md-12 col-md-offset-3 themeText">{{text}}</p>
    </div>
    <div class="container">
        <div class="text-center page-header">
            <h2 class="">{{title2}}</h2>
            <div class="lead">{{lead}}</div>
            <div class="subLead">{{subLead}}</div>
        </div>
        <div class="panel-body col-md-12 mobi-clear-padding features" style="background-color:rgb(99, 102, 106)">
            <ul class="col-md-12 col-xs-12 mobi-clear-padding clear-padding custom-icon-list">
                <h3 class="col-md-12 leftTitle">{{contentTitle}}</h3>
                {{#each custom }}
                <li class="col-md-2 col-sm-4 col-xs-6 text">
                    <i class="icon {{icon}}"></i>
                    <span class="title">{{title}}</span>
                    <span class="hidden-xs desc">{{desc}}</span>
                </li>
                {{/each}}
            </ul>
        </div>
        <div class="panel-body col-sm-12 col-md-12 mobi-clear-padding right-list" style="background-color:#f3f3f3">

            <div class="col-sm-6 col-md-6">
                <h3>{{title3}}</h3>
                <p class="text">{{text2}}</p>
            </div>
            <div class="col-sm-6 col-md-6">
                <h3>{{title4}}</h3>
                <p class="text">
                    <span>{{HPen}}
                        <a href="{{ENHref}}">{{ENHref}}</a>
                    </span>
                </p>
                <p class="text">
                    <span>{{HPcn}}
                        <a href="{{CNHref}}">{{CNHref}}</a>
                    </span>
                </p>
            </div>
        </div>
    </div>
</div>